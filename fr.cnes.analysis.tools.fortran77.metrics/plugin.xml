<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="fortran77.metrics"
         name="Fortran 77 Metrics"
         point="fr.cnes.analysis.tools.analyzer.checks">
      <check
            class="fr.cnes.analysis.tools.fortran77.metrics.F77METComplexitySimplified"
            id="fr.cnes.analysis.tools.fortran77.metrics.F77METComplexitySimplified"
            isMetric="true"
            languageId="fr.cnes.analysis.tools.languages.f77"
            name="F77.MET.ComplexitySimplified">
      </check>
      <check
            class="fr.cnes.analysis.tools.fortran77.metrics.F77METNesting"
            id="fr.cnes.analysis.tools.fortran77.metrics.F77METNesting"
            isMetric="true"
            languageId="fr.cnes.analysis.tools.languages.f77"
            name="F77.MET.Nesting">
      </check>
      <check
            class="fr.cnes.analysis.tools.fortran77.metrics.F77METLineOfCode"
            id="fr.cnes.analysis.tools.fortran77.metrics.F77METLineOfCode"
            isMetric="true"
            languageId="fr.cnes.analysis.tools.languages.f77"
            name="F77.MET.LineOfCode">
      </check>
      <check
            class="fr.cnes.analysis.tools.fortran77.metrics.F77METRatioComment"
            id="fr.cnes.analysis.tools.fortran77.metrics.F77METRatioComment"
            isMetric="true"
            languageId="fr.cnes.analysis.tools.languages.f77"
            name="F77.MET.RatioComment">
      </check>
   </extension>
</plugin>
