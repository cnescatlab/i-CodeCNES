      <plugin id="fr.cnes.analysis.tools.fortran77.rules.test" fragment="true"/>
      <plugin id="fr.cnes.analysis.tools.fortran77.ui"/>
      <plugin id="fr.cnes.analysis.tools.fortran90.analyzer"/>
