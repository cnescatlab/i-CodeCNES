<?xml version="1.0" encoding="UTF-8"?>
<?pde version="3.5"?>

<product name="i-Code CNES Command Line product" uid="icode.product" id="fr.cnes.icode.application.product" application="fr.cnes.icode.application.application" version="3.1.0" useFeatures="true" includeLaunchers="true">

